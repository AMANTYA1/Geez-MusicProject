{
  "name": "Geez-MusicProject",
  "description": "Open-Source bot to play songs in your Telegram's Group Voice Chat. Powered by PyTgCalls.",
  "keywords": ["music", "voicechat", "telegram"],
  "repository": "https://github.com/Vckyou/Geez-MusicProject",
  "stack": "container",
  "env": {
    "SESSION_NAME": {
      "description": "Gunakan String Pyrogram Jangan Telethon",
      "required": true
    },
    "BOT_TOKEN": {
      "description": "Ambil Token Bot Di @BotFather",
      "required": true
    },
    "BOT_USERNAME": {
      "description": "Masukan Username Bot Dan Gunakan @",
      "required": true
    },
    "BOT_NAME": {
      "description": "Nama Bot MusicPlayer Anda.",
      "required": false,
      "value": ""
    },
    "SUPPORT_GROUP": {
      "description": "Grup dukungan MusicPlayer Anda tanpa @ [Tinggalkan ini jika Anda tidak memilikinya]",
      "required": false,
      "value": "VcgSupportGroup"
    },
    "PROJECT_NAME": {
      "description": "Nama proyek MusicPlayer Anda..",
      "required": false,
      "value": "Geez Music Project"
    },
    "ASSISTANT_NAME": {
      "description": "Isi Nama pengguna asisten MusicPlayer Anda tanpa @.",
      "required": true
    },
    "ARQ_API_KEY": {
      "description": "Ambil Token ARQ Di @ARQrobot.",
      "required": true
    },    
    "BG_IMAGE": {
      "description": "BG image of bot (Use transparent one) [IGNORE].",
      "required": false,
      "value": "https://telegra.ph/file/ad54dc991d9e41dbbe37d.png"
    },     
    "UPDATES_CHANNEL": {
      "description": "Updates Channel without @ [Leave this if you don't have one].",
      "required": false,
      "value": "DaisyXupdates"
    },    
    "API_ID": {
      "description": "App ID from my.telegram.org/apps",
      "required": true
    },
    "API_HASH": {
      "description": "App hash from my.telegram.org/apps",
      "required": true
    },
    "SUDO_USERS": {
      "description": "List of user IDs counted as admin everywhere (separated by space).",
      "required": true
    },
    "DURATION_LIMIT": {
      "description": "Max audio duration limit for downloads (minutes).",
      "required": true,
      "value": "30"
    }
  }
}
